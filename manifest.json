{
   "action": {
      "default_title": "__MSG_app_name__"
   },
   "author": {
      "email": "convertwebpagetopdf@gmail.com"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_security_policy": {
      "extension_pages": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; font-src 'self' data:;"
   },
   "default_locale": "en",
   "description": "__MSG_app_description_short__",
   "icons": {
      "128": "/icon/128.png",
      "16": "/icon/16.png",
      "32": "/icon/32.png",
      "48": "/icon/48.png",
      "96": "/icon/96.png"
   },

   "manifest_version": 3,
   "name": "__MSG_app_name__",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "contextMenus", "downloads", "activeTab", "scripting", "debugger", "storage" ],
   "version": "0.0.1",
   "web_accessible_resources": [
    {
      "matches": ["*://*/*"],
      "resources": ["content-scripts/content.css", "icon/48.png"]
    }
  ]
}
